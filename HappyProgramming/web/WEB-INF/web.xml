<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>controller.AdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CVController</servlet-name>
        <servlet-class>controller.CVController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RatingController</servlet-name>
        <servlet-class>controller.RatingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestController</servlet-name>
        <servlet-class>controller.RequestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SkillController</servlet-name>
        <servlet-class>controller.SkillController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserController</servlet-name>
        <servlet-class>controller.UserController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminControllerMap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CVController</servlet-name>
        <url-pattern>/CVControllerMap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RatingController</servlet-name>
        <url-pattern>/RatingControllerMap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestController</servlet-name>
        <url-pattern>/RequestControllerMap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SkillController</servlet-name>
        <url-pattern>/SkillControllerMap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserController</servlet-name>
        <url-pattern>/UserControllerMap</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
